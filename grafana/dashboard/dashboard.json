          "pluginVersion": "4.7.0",
          "queryType": "table",
          "rawSql": "WITH status_counts AS (\r\n  SELECT \r\n    sensor_id,\r\n    sum(if(status = 'Normal', 1, 0)) AS Normal,\r\n    sum(if(status = 'Critical', 1, 0)) AS Critical,\r\n    sum(if(status NOT IN ('Normal', 'Critical'), 1, 0)) AS Other\r\n  FROM sensor_logs_db.sensor_logs\r\n  WHERE $__timeFilter(timestamp)\r\n  GROUP BY sensor_id\r\n)\r\nSELECT\r\n  sensor_id,\r\n  Normal,\r\n  Critical,\r\n  Other\r\nFROM status_counts\r\nORDER BY sensor_id ASC;\r\n",
          "refId": "A"
        }
      ],
      "title": "Sensor Data Distribution by Status",
      "type": "piechart"
    },
