          },
          "pluginVersion": "4.7.0",
          "queryType": "table",
          "rawSql": "WITH sensor_stats AS (\r\n  SELECT\r\n    sensor_id,\r\n    count(*) AS total_count,\r\n    sum(if(status = 'Normal', 1, 0)) AS normal_count\r\n  FROM sensor_logs_db.sensor_logs\r\n  WHERE $__timeFilter(timestamp)\r\n  GROUP BY sensor_id\r\n)\r\nSELECT\r\n  sensor_id,\r\n  (normal_count / CAST(total_count AS Float64)) * 100 AS percentage_normal\r\nFROM sensor_stats\r\nORDER BY sensor_id ASC",
          "refId": "A"
        }
      ],
      "title": "Normal Percentage for each Sensor",
      "type": "gauge"
    },
